{
  "name": "@kikiutils/mongoose",
  "type": "module",
  "version": "5.0.1",
  "description": "A Mongoose plugin for enhanced JSON normalization and common schema creation, with built-in support for pagination and automatic Decimal128 conversion.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/node-mongoose.git"
  },
  "keywords": [
    "Decimal128",
    "json",
    "mongo",
    "mongoose",
    "normalization",
    "pagination",
    "plugin",
    "schema"
  ],
  "sideEffects": false,
  "exports": {
    "./constants": "./dist/constants.js",
    "./options": "./dist/options.js",
    "./plugins/normalize": "./dist/plugins/normalize.js",
    "./schema-builders": "./dist/schema-builders/index.js",
    "./schema-builders/base": "./dist/schema-builders/base.js",
    "./schema-builders/boolean": "./dist/schema-builders/boolean.js",
    "./schema-builders/date": "./dist/schema-builders/date.js",
    "./schema-builders/decimal128": "./dist/schema-builders/decimal128.js",
    "./schema-builders/number": "./dist/schema-builders/number.js",
    "./schema-builders/object-id": "./dist/schema-builders/object-id.js",
    "./schema-builders/ref": "./dist/schema-builders/ref.js",
    "./schema-builders/string": "./dist/schema-builders/string.js",
    "./types": "./dist/types/index.js",
    "./types/aggregate-paginate": "./dist/types/aggregate-paginate.js",
    "./types/data": "./dist/types/data.js",
    "./types/options": "./dist/types/options.js",
    "./types/paginate": "./dist/types/paginate.js",
    "./types/utils": "./dist/types/utils.js",
    "./utils": "./dist/utils.js",
    "./package.json": "./package.json"
  },
  "files": [
    "./dist",
    "./src"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "depcheck": "depcheck",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --fix --max-warnings 0",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "typecheck": "tsc -b --noEmit",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "dependencies": {
    "decimal.js": "^10.6.0",
    "es-toolkit": "^1.39.10",
    "mongoose": "^8.18.1",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
    "mongoose-paginate-v2": "^1.9.1",
    "type-fest": "^5.0.0"
  },
  "devDependencies": {
    "@kikiutils/changelogen": "^0.9.0",
    "@kikiutils/eslint-config": "^3.0.1",
    "@kikiutils/tsconfigs": "^5.0.5",
    "@types/node": "^24.4.0",
    "@vitest/coverage-v8": "^3.2.4",
    "depcheck": "^1.4.7",
    "publint": "^0.3.12",
    "ts-unused-exports": "^11.0.1",
    "tsconfck": "^3.1.6",
    "tsdown": "^0.15.1",
    "typescript": "^5.9.2",
    "unplugin-unused": "^0.5.3",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
