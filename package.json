{
  "name": "@kikiutils/mongoose",
  "type": "module",
  "version": "7.0.1",
  "description": "A Mongoose plugin for enhanced JSON normalization and common schema creation, with built-in support for pagination and automatic Decimal128 conversion.",
  "author": "kiki-kanri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikiutils/node-mongoose.git"
  },
  "keywords": [
    "Decimal128",
    "json",
    "mongo",
    "mongoose",
    "normalization",
    "pagination",
    "plugin",
    "schema"
  ],
  "sideEffects": false,
  "exports": {
    "./_internals": {
      "types": null,
      "import": null,
      "require": null
    },
    "./package.json": "./package.json",
    "./schema-builders": {
      "types": "./dist/schema-builders/index.d.ts",
      "import": "./dist/schema-builders/index.js",
      "require": null
    },
    "./schema-builders/index": {
      "types": null,
      "import": null,
      "require": null
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": null,
      "require": null
    },
    "./types/_internals": {
      "types": null,
      "import": null,
      "require": null
    },
    "./types/index": {
      "types": null,
      "import": null,
      "require": null
    },
    "./*": "./dist/*.js"
  },
  "files": [
    "./dist",
    "./src"
  ],
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "tsdown",
    "bumplog": "changelogen --bump --hideAuthorEmail",
    "depcheck": "depcheck",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --fix --max-warnings 0",
    "prepack": "pnpm run build",
    "release": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build && changelogen --hideAuthorEmail --push --release && npm publish",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "typecheck": "tsc -b --noEmit",
    "unused-exports": "ts-unused-exports ./tsconfig.json"
  },
  "dependencies": {
    "decimal.js": "^10.6.0",
    "es-toolkit": "^1.44.0",
    "mongoose": "^9.1.4",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
    "mongoose-paginate-v2": "^1.9.1",
    "type-fest": "^5.4.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.1",
    "@kikiutils/eslint-config": "^5.0.2",
    "@kikiutils/tsconfigs": "^5.0.5",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^4.0.17",
    "changelogen": "^0.6.2",
    "depcheck": "^1.4.7",
    "publint": "^0.3.16",
    "ts-unused-exports": "^11.0.1",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3",
    "unplugin-unused": "^0.5.6",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
